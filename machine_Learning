if statement=>>
#if

a=13

b=14

c=20

if a>b :
    print('fucking a ')
else :
    print('b is greater than a')

if a>b & a>c :
    print('fucking a ')
elif b>a & b>c :
    print('b is greater ')
else :
    print('c is greater ')

tup=('a','b','c')

if 'a' in tup:
    print('a is present in tuple')
